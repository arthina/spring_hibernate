		// TODO Auto-generated method stub
		
		Configuration cfg=new Configurate().configure("hibernate.cfg.xml");
		ServiceRegistry serviceregisrty=new ServiceRegistryBuilder().applySetting(cfg.getProperties()).buildServiceRegistry();
		
		Session session=factory.openSession();
		Transaction t=session.beginTransaction();

	}

